{
  "name": "sandbox-es6",
  "version": "0.2.0",
  "description": "sandbox es6(2015)",
  "main": "",
  "scripts": {
    "start": "npm run serve & npm run watch",
    "build": "browserify src/basic.es6 --debug --transform babelify --outfile lib/basic.js",
    "lint": "eslint src/**/*.es6",
    "serve": "serve -p 3000 .",
    "watch:build": "watch 'npm run build' src/",
    "watch:lint": "watch 'npm run lint' src/",
    "watch": "npm run watch:build & npm run watch:lint",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DriftwoodJP/sandbox-es6.git"
