  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "eslint": "^2.7.0",
    "eslint-config-google": "^0.4.0",
    "serve": "^1.4.0",
    "watch": "^0.17.1"
  }
