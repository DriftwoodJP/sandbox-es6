{
  "name": "sandbox-es6",
  "version": "0.2.6",
  "description": "sandbox es6(2015)",
  "main": "",
  "scripts": {
    "start": "parallelshell 'npm run serve' 'npm run build' 'npm run watch'",
    "build": "watchify ./src/basic.es6.js --debug --transform babelify --outfile 'exorcist ./lib/basic.js.map > ./lib/basic.js' --verbose",
    "doc": "esdoc -c .esdoc.json",
    "lint": "eslint src/ test/",
    "serve": "serve -p 3000 .",
    "test": "mocha test/**/*.test.js",
    "watch": "watch 'npm run lint && npm run test' src/ test/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DriftwoodJP/sandbox-es6.git"
  },
  "author": "DriftwoodJP",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DriftwoodJP/sandbox-es6/issues"
  },
  "homepage": "https://github.com/DriftwoodJP/sandbox-es6#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "esdoc": "^0.4.8",
    "eslint": "^3.9.1",
    "eslint-config-google": "^0.7.0",
    "exorcist": "^0.4.0",
    "mocha": "^3.1.2",
    "parallelshell": "^2.0.0",
    "power-assert": "^1.4.1",
    "serve": "^1.4.0",
    "watch": "^1.0.1",
    "watchify": "^3.7.0"
  }
}
